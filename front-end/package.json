{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"{src,test}/**/*.{js,ts,json,tsx,jsx}\" --fix",
    "lint:check": "eslint \"{src,test}/**/*.{js,ts,json,tsx,jsx}\" ",
    "format": "prettier --write \"(src|test)/**/*.(js|ts|json|tsx|jsx|css|scss)\"",
    "format:check": "prettier --check \"(src|test)/**/*.(js|ts|json|tsx|jsx|css|scss)\"",
    "type:check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.1.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "@eslint/eslintrc": "^3"
  }
}
